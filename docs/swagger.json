{
    "swagger": "2.0",
    "info": {
        "description": "This is the documentation for the golang api of fizzbuzz-leboncoin",
        "title": "FizzBuzz LeBonCoin API",
        "contact": {
            "name": "API Support",
            "email": "pierre.saintsorny@gmail.com"
        },
        "version": "1"
    },
    "basePath": "/",
    "paths": {
        "/": {
            "get": {
                "summary": "Index of the api",
                "responses": {
                    "200": {
                        "description": ""
                    }
                }
            }
        },
        "/fizzbuzz": {
            "get": {
                "produces": [
                    "application/json"
                ],
                "summary": "Replace int1 and int2 by str1 and str2 for x in limit % int1 and int2",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "3",
                        "name": "int1",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "integer",
                        "description": "5",
                        "name": "int2",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "integer",
                        "description": "100",
                        "name": "limit",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "fizz",
                        "name": "str1",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "buzz",
                        "name": "str2",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/models.APIError"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/models.APIError"
                        }
                    }
                }
            }
        },
        "/statistics": {
            "get": {
                "summary": "Get the name of the max hit number of requests for a fizzbuzz",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/handlers.RequestStatisticsOutput"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/models.APIError"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "handlers.RequestStatisticsOutput": {
            "type": "object",
            "properties": {
                "hit_number": {
                    "type": "integer"
                },
                "int1": {
                    "type": "integer"
                },
                "int2": {
                    "type": "integer"
                },
                "limit": {
                    "type": "integer"
                },
                "str1": {
                    "type": "string"
                },
                "str2": {
                    "type": "string"
                }
            }
        },
        "models.APIError": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer"
                },
                "msg": {
                    "type": "string"
                }
            }
        }
    }
